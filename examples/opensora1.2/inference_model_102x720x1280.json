
{
    "ae": {
        "model_id": "3dvae",
        "from_pretrained": "OpenSora-VAE-v1.2/model.safetensors",
        "dtype": "bf16",
        "cal_loss": false,
        "shift": [-0.10, 0.34, 0.27, 0.98],
        "scale": [3.85, 2.32, 2.33, 3.06],
        "micro_frame_size" :17,
        "freeze_vae_2d": false
    },
    "text_encoder": {
       "hub_backend": "hf",
        "model_id": "T5",
        "from_pretrained": "DeepFloyd/t5-v1_1-xxl",
