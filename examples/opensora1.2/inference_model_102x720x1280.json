        "low_cpu_mem_usage": true,
        "dtype": "fp32"
    },
    "tokenizer":{
        "hub_backend": "hf",
        "autotokenizer_name": "AutoTokenizer",
        "from_pretrained": "DeepFloyd/t5-v1_1-xxl",
        "local_files_only": false
    },
    "predictor": {
        "dtype": "bf16",
        "model_id": "stdit3",
        "hidden_size" : 1152,
        "num_heads": 16,
